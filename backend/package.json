{
  "name": "vidify-backend",
  "version": "2.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node src/app.js",
    "dev": "tsx watch -r tsconfig-paths/register src/app.ts",
    "swagger": "tsx watch -r tsconfig-paths/register src/http/swagger.ts",
    "knex": "npx tsx ./node_modules/knex/bin/cli.js",
    "knex:migrate:make": "npm run knex migrate:make",
    "knex:migrate:latest": "npm run knex migrate:latest",
    "knex:migrate:rollback": "npm run knex migrate:rollback",
    "knex:migrate:status": "npm run knex migrate:status",
    "knex:seed:run": "npm run knex seed:run",
    "compose": "docker-compose -f docker-compose.dev.yaml up --build -d"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.370.0",
    "@aws-sdk/client-s3": "^3.378.0",
    "ass-compiler": "^0.1.14",
    "axios": "^1.7.7",
    "bullmq": "^5.13.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^11.2.0",
    "ioredis": "^5.4.1",
    "jimp": "^0.22.12",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "openai": "^4.52.0",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "socket.io": "^4.7.2",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.3",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.15.7",
    "typescript": "^5.2.2"
  }
}